<%- include('header_connected') %>

    <div id="inputContainer" class="container w-50 p-5">
        <div class="mb-4 text-center display-4">
            <h1 >Ajouter une Pathologie</h1>
        </div>
        <form action="/pathologie/ajouter" method="post" enctype="application/x-www-form-urlencoded">
            <div class="row">
                <div class="input-group">
                    <span class="input-group-text" for="pathNom">Nom</span>
                    <input type="text" class="form-control" id="pathNom" name="pathNom">
                </div>
                <hr class="my-4">
                <div class="col-md-12 text-center">
                    <button type="submit" class="btn btn-success btn-block" id="btnAjouterPathologie">Ajouter</button>
                </div>
            </div>
        </form>
    </div>
    
<%- include('footer') %>